#include "civic.h"

export int main() {
    int a = 560;
    int b = 320;

	void gcd() {
        if (a > b) a = a - b;
        else b = b - a;
	}

	while(a != b) gcd();

    printInt(a);
    printNewlines(1);

    return 0;
}

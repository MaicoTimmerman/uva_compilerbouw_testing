 *
 * @brief  This file finds the factorial of a number in Civilised C using
 *         relatively free variables.
 */

#include "civic.h"

export int main() {
	int result = 1;
	int n = 10;

	void factorial() {
		result = result * n;
		n = n - 1;
	}

	while(n > 0) factorial();

	printInt(result);
	printNewlines(1);

	return 0;
}

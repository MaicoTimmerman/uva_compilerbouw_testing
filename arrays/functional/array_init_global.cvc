extern void printInt(int val);
extern void printSpaces(int num);
extern void printNewlines(int num);

void printArray2D(int[n, m] a) {
    for (int i = 0, n) {
        for (int j = 0, m) {
            printInt(a[i, j]);
            printSpaces(1);
        }
        printNewlines(1);
    }
}

int[2, 2] a = [[1, 2], [3, 4]];

export int main() {
    printArray2D(a);
    return 0;
}

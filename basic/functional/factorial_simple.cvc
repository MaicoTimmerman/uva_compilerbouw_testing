 *
 * @brief  This file finds the factorial of a number in Civilised C.
 */

#include "civic.h"

int factorial(int a) {
	if(a > 1) a = a * factorial(a - 1);
	return a;
}

export int main() {
	int n = 10;

	if(n > 0)
		n = factorial(n);

	printInt(n);
	printNewlines(1);

	return 0;
}

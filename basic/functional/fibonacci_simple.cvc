#include "civic.h"

export int main() {
    int a = 0;
    int b = 1;
    int n = 100;
    int c;

    while (n > 0) {
        printInt(a);
        printNewlines(1);
        c = a + b;
        a = b;
        b = c;
        n = n - 1;
    }

    return 0;
}

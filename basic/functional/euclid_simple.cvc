#include "civic.h"

int gcd(int a, int b) {
    if (b == 0) return a;
    return gcd(b, a % b);
}

export int main() {
    int a = 560;
    int b = 320;

	a = gcd(a, b);

	printInt(a);
	printNewlines(1);

	return 0;
}
